import 'package:cloud_firestore/cloud_firestore.dart';

class UserModal {
  final String uid;

  UserModal({
      required this.uid,
    });
}

class UserData {
  final String sugars;
  final String name;
  final int strength;

  UserData({
    required this.sugars,
    required this.name,
    required this.strength
  });
}